import std::*

def factorial (I64 -> I64) {
    dup 0 = if { drop 1 } else { dup 1 - factorial * }
}

def main {
    0 factorial println
    1 factorial println
    2 factorial println
    3 factorial println
    4 factorial println
    5 factorial println
    6 factorial println
    7 factorial println
}
